<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mypediatric">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.example.mypediatric.PediatrasFound"></activity>
        <activity android:name="com.example.mypediatric.PediatrasSearch" />
        <activity android:name="com.example.mypediatric.Pediatras" />
        <activity android:name="com.example.mypediatric.Registro" />
        <activity android:name="com.example.mypediatric.Login" />
        <activity android:name="com.example.mypediatric.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>